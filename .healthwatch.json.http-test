{
  "channels": [
    {
      "id": "http-test",
      "name": "HTTP Test",
      "type": "http",
      "url": "http://httpbin.org/status/200",
      "intervalSec": 60,
      "expect": {
        "status": [200]
      }
    },
    {
      "id": "https-test", 
      "name": "HTTPS Test",
      "type": "https",
      "url": "https://httpbin.org/status/200",
      "intervalSec": 60,
      "expect": {
        "status": [200]
      }
    }
  ]
  ,
  "statusBar": {
    "mode": "compact",
    "icons": {
      "online": "$(check)",
      "offline": "$(error)",
      "unknown": "$(question)"
    },
    "format": {
      "showLatency": false,
      "separator": ":",
      "maxChannelItems": 6,
      "order": "explicit"
    }
  }
}
